@model IEnumerable<BusinessObject.ItemSubType>
@{
    ViewBag.Title = "GetSubTypes";
}

<div class="w3-card-4 mr-2">
    <header class="w3-container w3-dark-gray">
            <div class="row">
                <div class="col-9"><h1>Sub Types</h1></div>
                <div class="col-3 text-right">
                    <a href="~/Configuration/AddSubType" class="w3-button w3-teal"><i class="fa fa-plus"></i> Add New</a>
                </div>
            </div>
    </header>

    <div class="w3-container">
        <p>
            <table class="table table-bordered table-striped">
                <thead>
                <tr>
                    <th>Control</th>
                    <th>Name</th>
                    <th>Type</th>
                    <th>Description</th>
                </tr>
                </thead>
                <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td></td>
                        <td>@item.Name</td>
                        <td></td>
                        <td>@item.Description</td>
                    </tr>
                }
                </tbody>
            </table>
        </p>
    </div>

</div>


